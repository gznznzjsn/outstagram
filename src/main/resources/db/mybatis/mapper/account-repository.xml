<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.gznznzjsn.outstagram.repository.AccountRepository">

    <resultMap id="AccountResultMap" type="Account" autoMapping="false">
<!--        <id column="account_id" property="id"/>-->
        <result column="account_name" property="name"/>
    </resultMap>

    <insert id="create" useGeneratedKeys="">
        create (
            a:ACCOUNT {
                        name: #{name}
            }
        )
        return a
    </insert>

</mapper>